---
import Contact from "@/components/forms/Contact.astro";
import BaseLayout from "@/layouts/BaseLayout.astro";
import type { AstroSeoProps } from "@astrolib/seo";

const seo: AstroSeoProps = {
	title: "Contacto Metarrelato: Solicita una cita o envía un correo electrónico",
	description:
		"Reach out to Metarrelato for expert web design and SEO services. Connect with us to discuss your project and explore cutting-edge web solutions.",
	canonical: "https://metarrelato.com/contact/",
};
---

<BaseLayout seo={seo}>
	<Contact />
</BaseLayout>
